<% if (success && success.length > 0) { %>
  <div class="alert alert-success">
    <i class="fas fa-check-circle"></i>
    <%= success %>
  </div>
<% } %>

<% if (error && error.length > 0) { %>
  <div class="alert alert-error">
    <i class="fas fa-exclamation-circle"></i>
    <%= error %>
  </div>
<% } %>
